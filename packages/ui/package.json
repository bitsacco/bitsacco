{
  "name": "@bitsacco/ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./tokens": {
      "types": "./dist/tokens/index.d.ts",
      "default": "./dist/tokens/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    }
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "tsc --project tsconfig.json --watch",
    "clean": "rm -rf dist",
    "lint": "eslint \"src/**/*.ts*\"",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": "*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@bitsacco/eslint-config": "*",
    "@bitsacco/tsconf": "*",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.6",
    "eslint": "^8.57.0",
    "next": "^15.1.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "5.5.4"
  }
}
