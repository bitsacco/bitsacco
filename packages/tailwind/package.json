    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@bitsacco/tsconf": "*",
    "@types/node": "^22.8.7",
    "typescript": "5.5.4"
  }
}
